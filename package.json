{
	"name": "virtual-document-python-demo",
	"displayName": "Virtual Document Python Demo",
	"description": "Virtual document demo for python's IntelliSense and hovering in markdown file.",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.54.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "12.11.7",
		"@types/vscode": "1.51.0",
		"@typescript-eslint/eslint-plugin": "4.1.1",
		"@typescript-eslint/parser": "4.1.1",
		"eslint": "7.9.0",
		"typescript": "4.0.2"
	}
}
